#include<stdio.h>
int f(int);
int main()
{
	int n,m=0,i;
	printf("enter total terms:");
	scanf("%d",&n);
	printf("fibonacci series terms are:");
	for(i=1;i<=n;i++)
	{
		printf("%d",fibonacci(m));
		m++;
	}
	return 0;
}
int fibonacci(int n)
{
	if(n==0||n==1)
	return n;
	else
	return(fibonacci(n-1)+fibonacci(n-2));
}
